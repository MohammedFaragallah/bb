{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"analyze": "yarn build:dev && source-map-explorer 'build/static/js/*.js'",
		"build": "cross-env GENERATE_SOURCEMAP=false yarn run build:dev",
		"build:dev": "react-scripts build",
		"dev": "react-scripts start",
		"eject": "react-scripts eject",
		"extract": "cross-env NODE_ENV=development extract/index.js -l=en,ar -o src/localization/messages -d en --flat false 'src/**/!(*.test).js' --keep",
		"lint": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
		"push": "git add . && git commit -m 'save' && git push heroku master",
		"serve": "serve -s build",
		"start": "react-scripts start",
		"test": "react-scripts test",
		"update": "yarn upgrade-interactive"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --config .prettierrc --write",
			"git add"
		]
	},
	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
